noremap -b -m v,i,t,c,o <C-c> <Esc>
noremap -b -m v,i,t,c,o <C-]> <Esc>
noremap -b h 20j
noremap -b t 20k
noremap -b k t
noremap -b j d
noremap -b K T
noremap -b J D
noremap -b ; :
noremap -b : ;
noremap -b <C-j> <C-d>
noremap -b d h
noremap -b n l
noremap -b l n
noremap -b d :tp<CR>
noremap -b n :tn<CR>
noremap -b D :tabmove -1<CR>
noremap -b N :tabmove +1<CR>
noremap -b , @:

colorscheme solarized-dark

js editor.defaultRegister="+"

set defsearch=google
set hintkeys="aoeuidhtns"
set guioptions=bCrsn

noremap -m i <C-p> <C-v><C-v>
noremap -m i <C-y> <C-v><C-c>
noremap -m i <C-a> <C-v><C-a>

noremap -m i <C-e> <C-i>

noremap -ex gm :js CommandExMode().open("!youtube-play '" + content.location + "'")

set editor='emacsclient -c'