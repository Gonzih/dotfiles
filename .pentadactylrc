map -b -m v,t,c,o <C-c> <Esc>
map -b -m v,i,t,c,o <C-]> <Esc>
map -b h 5j
map -b t 5k
map -b k t
map -b j d
map -b K T
map -b J D
map -b ; :
map -b : ;
map -b <C-j> <C-d>
map -b d h
map -b n l
map -b l n
map -b d :tp<CR>
map -b n :tn<CR>
map -b D :tabmove -1<CR>
map -b N :tabmove +1<CR>
map -b , @:

colorscheme solarized-dark

js editor.defaultRegister="+"

set defsearch=google
set hintkeys="aoeuidhtns"
set guioptions=bCrs

map -m i <C-p> <C-v><C-v>
map -m i <C-y> <C-v><C-c>
map -m i <C-a> <C-v><C-a>

map -m i <C-e> <C-i>

map -ex gm :js CommandExMode().open("!youtube-play '" + content.location + "'")

set! extensions.pterosaur.exitkey "<C-c>"
set! extensions.pterosaur.contentonly true
